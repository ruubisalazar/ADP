lista_numeros = [1, 3, 6, 15, 35]

numero_buscar = int(input("Ingrese un número para buscar en la lista: "))

if numero_buscar in lista_numeros:
    print(f"El número {numero_buscar} está presente en la lista.")
else:
    print(f"El número {numero_buscar} no está en la lista.")
